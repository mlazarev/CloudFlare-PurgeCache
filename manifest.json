{
  "manifest_version": 3,
  "name": "CloudFlare Tools",
  "version": "1.1.0",
  "description": "One-click cache purge plus access rule controls for the current site",
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "host_permissions": [
    "https://api.cloudflare.com/*",
    "https://api.ipify.org/*"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
